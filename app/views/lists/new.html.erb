<h1>新規投稿</h1>

<%= render 'layouts/errors', obj: @list %>

<%= form_with model: List.new do |f| %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>本文</h4>
  <%= f.text_area :body %>

  <h4>画像</h4>
  <%= f.file_field :list_images, multiple: true, id: 'list_images' %>

  <%= f.submit '投稿' %>

<% end %>
