<h2>タイトル</h2>
<p><%= @list.title %></p>

<h2>本文</h2>
<p><%= @list.body %></p>

<% if @list.list_images.attached? %>
  <% @list.list_images.each do |image| %>
    <%= image_tag image, size: "200x200" %> <br>
  <% end %>
<% end %>

<%= link_to "編集", edit_list_path(@list.id) %>
<%= link_to "削除", list_path(@list.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>